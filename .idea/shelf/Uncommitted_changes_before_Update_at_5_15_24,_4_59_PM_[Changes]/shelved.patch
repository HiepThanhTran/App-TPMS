+            raise ValidationError({'detail': 'Không thể xóa báo thiếu đã được giải quyết'})
+
+        report.delete()
+        return Response(status=status.HTTP_204_NO_CONTENT)
+
+    @swagger_schema.missing_reports_list_schema()
